{
    "name": "dynamic/silverstripe-foxy-single-sign-on",
    "description": "A skeleton for SilverStripe modules.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "dynamic/dynamic/silverstripe-foxy-api": "1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Foxy\\SingleSignOn\\": "src/",
            "Dynamic\\Foxy\\SingleSignOn\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:dynamic/silverstripe-foxy-api.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
